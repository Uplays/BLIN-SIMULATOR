{
  "name": "blin-os",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://uplays.github.io/BLIN-SIMULATOR",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "cp index.html dist/index.html && esbuild index.jsx --bundle --outfile=dist/bundle.js --loader:.jsx=jsx --define:process.env.NODE_ENV=\\\"production\\\" --minify",
    "deploy": "npm run build && gh-pages -d dist",
    "start": "esbuild index.jsx --bundle --outfile=dist/bundle.js --loader:.jsx=jsx --servedir=dist --watch"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "gh-pages": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.3"
  }
}